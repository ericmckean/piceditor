<?xml version="1.0" encoding="utf-8"?>
<mx:HBox xmlns:mx="http://www.adobe.com/2006/mxml" xmlns:controls="controls.*" width="100%" height="100%" paddingLeft="30" paddingRight="20" horizontalGap="30">
	

<mx:Script>
	<![CDATA[
		[Bindable] public var area:String = "shapes";
	]]>
</mx:Script>

	<mx:DropShadowFilter id="_fiWhiteShine" blurX="1" blurY="1" distance="1" color="#FFFFFF" alpha="1" quality="3" angle="90"/>
	<mx:DropShadowFilter id="_fiInnerShad" blurX="1" blurY="1" distance="1" color="#000000" alpha=".2" quality="3" angle="90" inner="true"/>

			<!--Seasonal Stickers-->
			<mx:VBox width="100%" height="100%" horizontalAlign="center" paddingTop="10" clipContent="false">
				<controls:ImageVer source="../graphics/upsell/shapes_holiday.png"/>
				<mx:VBox width="100%" verticalGap="-8">
				<controls:LabelPlus htmlText="@Resource(key='SeasonalHeader', bundle='ShapesUpsellBox')" styleName="targetedUpsellHead" width="100%" autoFontSizeGap="1" selectable="false"/>
				<controls:TextPlus  htmlText="@Resource(key='SeasonalCopy', bundle='ShapesUpsellBox')" styleName="targetedUpsellSubhead" width="100%" selectable="false"/>											
				</mx:VBox>
			</mx:VBox>
			<!--Exclusive Stickers-->
			<mx:VBox width="100%" height="100%" horizontalAlign="center" paddingTop="10" clipContent="false">
				<controls:ImageVer source="../graphics/upsell/shapes_premexclu.png" horizontalAlign="center"/>
				<mx:VBox width="100%" verticalGap="-8">
				<controls:LabelPlus htmlText="@Resource(key='ExclusiveHeader', bundle='ShapesUpsellBox')" styleName="targetedUpsellHead" width="100%" autoFontSizeGap="1" selectable="false"/>
				<controls:TextPlus  htmlText="@Resource(key='ExclusiveCopy', bundle='ShapesUpsellBox')" styleName="targetedUpsellSubhead" width="100%" selectable="false"/>											
				</mx:VBox>
			</mx:VBox>
</mx:HBox>